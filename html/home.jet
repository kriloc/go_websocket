<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Homw</title>
</head>
<body>
<h1>HomePage</h1>
<script>
    let socket = null;

    document.addEventListener("DOMContentLoaded", function () {
        socket = new WebSocket("ws://127.0.0.1:8080/ws");

        socket.onopen = () => {
            console.log("Successfully connected");
        }
    })
</script>
</body>
</html>
